{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Nicholas Ketchum",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "js-cookie/js-cookie",
                "version": "2.1.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/js-cookie/js-cookie/archive/v2.1.4.zip",
                    "type": "zip"
                 }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.5.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.5.1/chosen_v1.5.1.zip",
                    "type": "zip"
                 }
            }
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "cweagans/composer-patches": "~1.0",
        "drupal-composer/drupal-scaffold": "~2.0",
        "drupal/address": "~1.0",
        "drupal/admin_toolbar": "~1.0",
        "drupal/block_class": "~1.0",
        "drupal/bootstrap": "~3.0",
        "drupal/chosen": "~2.0",
        "drupal/colorbox": "~1.0",
        "drupal/console": "~1.0",
        "drupal/core": "~8.0",
        "drupal/devel": "~1.0",
        "drupal/double_field": "~3.0",
        "drupal/entity": "~1.0",
        "drupal/field_group": "~1.0",
        "drupal/inline_entity_form": "~1.0",
        "drupal/menu_trail_by_path": "~1.0",
        "drupal/mollom": "~1.0",
        "drupal/pathauto": "~1.0",
        "drupal/range": "~1.0",
        "drupal/viewfield": "~3.0",
        "drush/drush": "~8.0",
        "harvesthq/chosen": "~1.0",
        "jackmoore/colorbox": "master",
        "js-cookie/js-cookie": "~2.1.0",
        "drupal/flag": "4.x-dev"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/composer/bootstrap-subtheme.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "bootstrap-subtheme": [
            "DrupalProject\\composer\\SubthemeHandler::createSubtheme"
        ]
    },
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
